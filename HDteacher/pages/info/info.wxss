.studentInfo{
  width:94%;
  height:200rpx;
  border-bottom:0.5px solid #efefef;
  margin:0 auto;
  padding-bottom: 20rpx;
}
.studentInfoLogo{
  width: 130rpx;
  height: 130rpx;
  padding: 35rpx 35rpx;
  float: left;
}
.studentInfoLogo image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.studentInfoDetail{
  width: 260rpx;
  padding: 45rpx 20rpx;
  font-size: 24rpx;
  line-height: 40rpx;
  float: left;
}
.studentInfoDetail view:nth-child(1){
  font-size: 28rpx;
}
navigator{
  width:92%;
  padding-left: 2%;
  height:100rpx;
  font-size: 32rpx;
  text-align: left;
  border-bottom:0.5px solid #efefef;
  margin:0 auto;
  color: #555;
  line-height: 100rpx;
  position: relative;
}
navigator image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top:25rpx;
  right: 10rpx;
}