.topTitle{
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 20rpx;
}
.center{
  text-align: center;
}
.topTitle text{
  margin: 0px 20rpx;
}
.attendanceInfo{
  font-size: 24rpx;
}
.searchLabel{
  display: inline-block;
  font-size: 28rpx;
}
picker image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}
picker{
  margin:50rpx 50rpx 0rpx 50rpx;
  position:relative;
  border-bottom:1rpx solid #efefef;
  line-height:60rpx;
  padding:0px 10rpx;

}
/* 表格 */
.table {
  border: 0px solid darkgray;
  font-size: 28rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 1.8rem;
  align-items: center;
  padding:10rpx 0rpx;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #1ba0e7;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
}